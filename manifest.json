
{
	"name": "Times Bandit",
	"description": "Clear local storage, session storage, and session cookies",
	"version": "2.0",
	"permissions": [
		"activeTab",
		"tabs",
		"storage",
		"cookies",
		"*://*.google.com/*",
		"*://*.nytimes.com/*"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Click to clear local storage, session storage, and session cookies"
	},
